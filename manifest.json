{
  "name": "Hermes",
  "version": "1.0",
  "description": "Remove spotify ad.",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["script.js"]
    }
  ],
  "manifest_version": 2
}
